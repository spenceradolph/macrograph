{
  "compilerOptions": {
    "target": "ES5",
    "module": "ES6",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "out",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "strict": true,
    "rootDir": "src",
    "importHelpers": true
  },
  "include": ["src/electron"]
}
